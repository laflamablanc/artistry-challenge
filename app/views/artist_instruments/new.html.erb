<h3> Artist Form </h3>

<% <%= form_for @artist_instrument do |f| %>
  <%= f.label :artist %>
  <%= f.collection_select :artist, Artist.all, :id, :name %>
  <br>
  <%= f.label :instrument %>
  <%= f.collection_select :instruments, Instrument.all, :id, :name %>
  <br>
  <br>
  <%= f.submit %>

<% end %> %>
