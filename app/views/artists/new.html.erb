<h3> Artist Form </h3>

<%= form_for @artist do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :instrument %>
  <%= f.collection_select :instruments, Instrument.all, :id, :name %>
  <br>
  <br>
  <%= f.submit %>

<% end %>
